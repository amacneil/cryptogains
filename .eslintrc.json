{
  "extends": "airbnb-base",
  "rules": {
    "arrow-body-style": "off",
    "comma-dangle": ["error", "always-multiline"],
    "new-cap": "off",
    "no-await-in-loop": "off",
    "no-console": "off",
    "no-continue": "off",
    "no-param-reassign": ["error", { "props": false }],
    "no-restricted-syntax": [
      "error",
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "prefer-destructuring": ["error", {"object": true, "array": false}],
    "strict": "off"
  }
}

